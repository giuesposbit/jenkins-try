pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "Build step1"'
                sh 'echo "Build step2"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Test step1"'
                sh 'echo "Test step2"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploy step1"'
                sh 'echo "Deploy step2"'
            }
        }
    }
}
